{
  "name": "users",
  "version": "1.1.0",
  "description": "Starter pack for an auth-included graphql project",
  "repository": {
    "type": "git",
    "url": "github.com/stephengrider"
  },
  "private": true,
  "scripts": {
    "dev": "concurrently \"npm run build:server -- --watch\" \"npm run build:client -- --watch\" \"nodemon dist/server/server.js\"",
    "start": "node dist/server/server.js",
    "clean": "rm -rf dist",
    "build": "npm run clean && npm run build:server && npm run build:client",
    "build:client": "webpack --config webpack.config.client.js --profile --json > statsClient.json",
    "build:server": "webpack --config webpack.config.server.js --profile --json > statsServer.json",
    "test": "jest"
  },
  "author": "Ilya Sevrugin",
  "license": "ISC",
  "dependencies": {
    "apollo-client": "^0.8.2",
    "apollo-link-http": "^1.5.16",
    "bcrypt-nodejs": "0.0.3",
    "body-parser": "^1.16.0",
    "connect-mongo": "^1.3.2",
    "express": "^4.17.1",
    "express-graphql": "^0.6.1",
    "express-session": "^1.15.0",
    "graphql": "^0.8.2",
    "graphql-tag": "^1.2.4",
    "lodash": "^4.17.4",
    "mongoose": "^5.6.11",
    "passport": "^0.3.2",
    "passport-local": "^1.0.0",
    "react": "^16.11.0",
    "react-apollo": "^0.10.0",
    "react-dom": "^15.4.2",
    "react-router": "^3.0.2",
    "redis": "^2.8.0"
  },
  "devDependencies": {
    "@babel/core": "^7.7.0",
    "@babel/preset-env": "^7.7.1",
    "@babel/preset-react": "^7.7.0",
    "babel-loader": "^8.0.6",
    "concurrently": "^5.0.0",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^24.9.0",
    "nodemon": "*",
    "react-test-renderer": "^16.11.0",
    "webpack": "^4.41.2",
    "webpack-bundle-analyzer": "^3.6.0",
    "webpack-cli": "^3.3.10",
    "webpack-node-externals": "^1.7.2"
  }
}
